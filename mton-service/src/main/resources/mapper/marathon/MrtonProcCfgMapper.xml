<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.marathon.mapper.MrtonProcCfgMapper">

    <resultMap type="MrtonProcCfg" id="MrtonProcCfgResult">
                    <result property="procId" column="proc_id"/>
                    <result property="procName" column="proc_name"/>
                    <result property="procSeq" column="proc_seq"/>
                    <result property="parentProcId" column="parent_proc_id"/>
                    <result property="comparativelyToMrton" column="comparatively_to_mrton"/>
                    <result property="starttimeOffset" column="starttime_offset"/>
                    <result property="endtimeOffset" column="endtime_offset"/>
