                    </if>
                                                                <if test="endtimeOffset != null  ">endtime_offset = #{endtimeOffset},
                    </if>
                                                                <if test="procContent != null  and procContent != ''  ">proc_content = #{procContent},
                    </if>
                                                                <if test="procDescri != null  and procDescri != ''  ">proc_descri = #{procDescri},
                    </if>
                                                                <if test="attachResourceId != null  and attachResourceId != ''  ">attach_resource_id = #{attachResourceId},
                    </if>
                                                                <if test="procStatus != null  ">proc_status = #{procStatus},
                    </if>
                                                                <if test="reserved != null  and reserved != ''  ">reserved = #{reserved},
                    </if>
                                                                <if test="delFlag != null  ">del_flag = #{delFlag},
                    </if>
                                    </trim>
        where proc_id = #{procId}
    </update>

    <delete id="deleteMrtonProcCfgById" parameterType="String">
        delete from mrton_proc_cfg where proc_id = #{procId}
    </delete>

    <delete id="deleteMrtonProcCfgByIds" parameterType="String">
        delete from mrton_proc_cfg where proc_id in
        <foreach item="procId" collection="array" open="(" separator="," close=")">
            #{procId}
        </foreach>
    </delete>

</mapper>